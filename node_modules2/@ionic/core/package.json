{
  "name": "@ionic/core",
  "version": "0.0.2-48",
  "description": "Base components for Ionic",
  "main": "dist/ionic.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
